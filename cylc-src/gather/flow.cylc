[scheduling]
    [[graph]]
        R1 = """
            a & b => c
        """
[runtime]
    [[a]]
        script = """
        echo "executing task A..."
        sleep 5
        echo "done with task A..."
        """
    [[b]]
        script = """
        echo "executing task B..."
        sleep 2
        echo "done with task B"
        """
    [[c]]
        script = """
        echo "executing task C..."
        sleep 1
        echo "done with task C"
        """

